#include <iostream>
#include <unordered_set>
using namespace std;

struct Node {
    int data;
    Node* left;
    Node* right;
    Node(int d) : data(d), left(NULL), right(NULL) {}
};

bool findDuplicate(Node* root, unordered_set<int>& s) {
    if (root == NULL)
        return false;

    if (s.find(root->data) != s.end())
        return true;

    s.insert(root->data);

    return findDuplicate(root->left, s) || findDuplicate(root->right, s);
}

int main() {
    Node* root = new Node(1);
    root->left = new Node(2);
    root->right = new Node(3);
    root->left->left = new Node(4);
    root->left->right = new Node(2);  // duplicate

    unordered_set<int> s;

    if (findDuplicate(root, s))
        cout << "Duplicates Found";
    else
        cout << "No Duplicates";

    return 0;
}
