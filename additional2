#include <iostream>
#include <unordered_set>
using namespace std;

bool pairSum(int arr[], int n, int target) {
    unordered_set<int> s;
    for (int i = 0; i < n; i++) {
        int needed = target - arr[i];
        if (s.find(needed) != s.end())
            return true;
        s.insert(arr[i]);
    }
    return false;
}

int main() {
    int arr[] = {0, -1, 2, -3, 1};
    int n = 5;
    int target = -2;

    cout << (pairSum(arr, n, target) ? "true" : "false");
    return 0;
}
