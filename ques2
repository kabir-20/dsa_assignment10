#include <iostream>
using namespace std;

#define MAX 100000

void printCommon(int A[], int n1, int B[], int n2) {
    int hash[MAX] = {0};
    for (int i = 0; i < n1; i++)
        hash[A[i]] = 1;

    for (int i = 0; i < n2; i++)
        if (hash[B[i]] == 1)
            cout << B[i] << " ";
}

int main() {
    int A[] = {1, 2, 3, 4};
    int B[] = {3, 4, 5, 6};
    printCommon(A, 4, B, 4);
    return 0;
}
